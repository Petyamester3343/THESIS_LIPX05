<?xml version="1.0" encoding="utf-8"?>
<batchML:BatchInformation xmlns:batchML="http://www.wbf.org/xml/BatchML-V02" xmlns:custom="http://lipx05.y0kai.com/batchml/custom">
  <batchML:MasterRecipe>
    <batchML:Header>
      <batchML:ID>FlowShop-MR-001</batchML:ID>
      <batchML:Version>1.0</batchML:Version>
      <batchML:Description>2-MACHINE FLOW SHOP MODEL (Optimized)</batchML:Description>
    </batchML:Header>
    <batchML:RecipeElement>
      <batchML:ID>Processing-Module</batchML:ID>
      <batchML:RecipeElementType>UnitProcedure</batchML:RecipeElementType>
      <batchML:Description>Job A: Mixing And Flavoring (M1/M2)</batchML:Description>
    </batchML:RecipeElement>
    <batchML:RecipeElement>
      <batchML:ID>Forming-Module</batchML:ID>
      <batchML:RecipeElementType>UnitProcedure</batchML:RecipeElementType>
      <batchML:Description>Job B: Dehydration And Chopping (M1/M2)</batchML:Description>
    </batchML:RecipeElement>
    <batchML:RecipeElement>
      <batchML:ID>Final-Module</batchML:ID>
      <batchML:RecipeElementType>UnitProcedure</batchML:RecipeElementType>
      <batchML:Description>Job C: Packaging and Deployment (M1/M2)</batchML:Description>
    </batchML:RecipeElement>
    <batchML:ProcedureLogic>
      <batchML:Step>
        <batchML:ID>Job_A</batchML:ID>
        <batchML:RecipeElementID>Mixing-Module</batchML:RecipeElementID>
        <batchML:Extension>
          <custom:TimeM1>25</custom:TimeM1>
          <custom:TimeM2>115</custom:TimeM2>
        </batchML:Extension>
      </batchML:Step>
      <batchML:Step>
        <batchML:ID>Job_B</batchML:ID>
        <batchML:RecipeElementID>Forming-Module</batchML:RecipeElementID>
        <batchML:Extension>
          <custom:TimeM1>75</custom:TimeM1>
          <custom:TimeM2>175</custom:TimeM2>
        </batchML:Extension>
      </batchML:Step>
      <batchML:Step>
        <batchML:ID>Job_C</batchML:ID>
        <batchML:RecipeElementID>Final-Module</batchML:RecipeElementID>
        <batchML:Extension>
          <custom:TimeM1>55</custom:TimeM1>
          <custom:TimeM2>20</custom:TimeM2>
        </batchML:Extension>
      </batchML:Step>
    </batchML:ProcedureLogic>
  </batchML:MasterRecipe>
</batchML:BatchInformation>